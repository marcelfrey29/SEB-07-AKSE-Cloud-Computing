{
    "name": "todo-frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start:dev": "nuxt --dotenv dev.env",
        "start:prod": "nuxt --dotenv production.env",
        "build:dev": "nuxt build --dotenv dev.env",
        "build:prod": "nuxt build --dotenv production.env",
        "generate:dev": "nuxt generate --fail-on-error --dotenv dev.env",
        "generate:prod": "nuxt generate --fail-on-error --dotenv production.env",
        "serve": "nuxt start",
        "lint": "npm run lint:prettier:fix",
        "lint:eslint:check": "eslint --ext \".js,.ts,.vue\" .",
        "lint:prettier:check": "prettier --check .",
        "lint:prettier:fix": "prettier --write .",
        "test": "npm run test:unit",
        "test:unit": "jest"
    },
    "dependencies": {
        "@nuxtjs/auth-next": "^5.0.0-1641466805.bcbca68",
        "@nuxtjs/axios": "^5.13.6",
        "bootstrap": "^4.6.1",
        "bootstrap-vue": "^2.21.2",
        "core-js": "^3.15.2",
        "nuxt": "^2.15.8",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.14.7",
        "@nuxt/test-utils": "^0.2.2",
        "@nuxt/types": "^2.15.7",
        "@nuxt/typescript-build": "^2.1.0",
        "@nuxtjs/eslint-config-typescript": "^6.0.1",
        "@nuxtjs/eslint-module": "^3.0.2",
        "@types/jest": "^27.0.2",
        "@vue/test-utils": "^1.2.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "^27.0.5",
        "eslint": "^7.29.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-nuxt": "^2.0.0",
        "eslint-plugin-vue": "^7.12.1",
        "jest": "^27.3.1",
        "prettier": "^2.3.2",
        "ts-jest": "^27.0.3",
        "vue-jest": "^3.0.4"
    }
}
